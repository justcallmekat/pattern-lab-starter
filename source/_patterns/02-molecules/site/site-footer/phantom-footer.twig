<!-- Footer -->

<div id="wrapper">
<footer id="footer">
  <div class="inner">
<!-- Form-->
    <section>
      <h2>{{footer_subhead1}}</h2>
      <form method="post" action="#">
        <div class="field half first">
          <input type="text" name="name" id="name" placeholder="Name"/>
        </div>
        <div class="field half">
          <input type="email" name="email" id="email" placeholder="Email"/>
        </div>
        <div class="field">
          <textarea name="message" id="message" placeholder="Message"></textarea>
        </div>
        <ul class="actions">
          <li><input type="submit" value="Send" class="special"/></li>
        </ul>
      </form>
    </section>

<!--Social media icons-->

    <section>
      <h2>{{footer_subhead2}}</h2>
      <ul class="icons">
        <li><a href="{{dummy_link}}" class="icon style2 fa-twitter"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-facebook"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-instagram"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-dribbble"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-github"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-500px"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-phone"><span class="label">{{icons_text}}</span></a></li>
        <li><a href="{{dummy_link}}" class="icon style2 fa-envelope-o"><span class="label">{{icons_text}}</span></a></li>
      </ul></section>

<!--Copywright-->
    <ul class="copyright">
      <li>&copy; {{copy_text}}</li>
      <li>{{copy_text}} <a href="{{html5_up}}">{{html5_up_text}}</a></li>
    </ul>
  </div>
</footer>
</div><!-- /#wrapper-->

